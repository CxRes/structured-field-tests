[
    {
      "name": "basic identifier - item",
      "raw": ["a_b-c.d3:f%00/*"],
      "header_type": "item",
      "expected": "a_b-c.d3:f%00/*"
    },
    {
      "name": "identifier with capitals - item",
      "raw": ["fooBar"],
      "header_type": "item",
      "expected": "fooBar"
    },
    {
      "name": "identifier starting with capitals - item",
      "raw": ["FooBar"],
      "header_type": "item",
      "expected": "FooBar"
    },
    {
      "name": "bad identifier - item",
      "raw": ["abc$%!"],
      "header_type": "item",
      "must_fail": true
    },
    {
      "name": "basic identifier - list",
      "raw": ["a_b-c3/*"],
      "header_type": "param-list",
      "expected": [["a_b-c3/*", {}]]
    },
    {
      "name": "identifier with capitals - list",
      "raw": ["fooBar"],
      "header_type": "param-list",
      "expected": [["fooBar", {}]]
    },
    {
      "name": "identifier starting with capitals - list",
      "raw": ["FooBar"],
      "header_type": "param-list",
      "expected": [["FooBar", {}]]
    },
    {
      "name": "identifier with bad start - list",
      "raw": ["*/*"],
      "header_type": "param-list",
      "must_fail": true
    },
    {
      "name": "bad identifier - list",
      "raw": ["abc$%!"],
      "header_type": "param-list",
      "must_fail": true
    }
]
