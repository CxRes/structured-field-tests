[
    {
      "name": "basic token - item",
      "raw": ["a_b-c.d3:f%00/*"],
      "header_type": "item",
      "expected": "a_b-c.d3:f%00/*"
    },
    {
      "name": "token with capitals - item",
      "raw": ["fooBar"],
      "header_type": "item",
      "expected": "fooBar"
    },
    {
      "name": "token starting with capitals - item",
      "raw": ["FooBar"],
      "header_type": "item",
      "expected": "FooBar"
    },
    {
      "name": "bad token - item",
      "raw": ["abc$%!"],
      "header_type": "item",
      "must_fail": true
    },
    {
      "name": "basic token - list",
      "raw": ["a_b-c3/*"],
      "header_type": "list",
      "expected": [["a_b-c3/*", {}]]
    },
    {
      "name": "token with capitals - list",
      "raw": ["fooBar"],
      "header_type": "list",
      "expected": [["fooBar", {}]]
    },
    {
      "name": "token starting with capitals - list",
      "raw": ["FooBar"],
      "header_type": "list",
      "expected": [["FooBar", {}]]
    },
    {
      "name": "bad token - list",
      "raw": ["abc$%!"],
      "header_type": "list",
      "must_fail": true
    }
]
