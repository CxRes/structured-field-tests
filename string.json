[
    {
      "name": "basic string",
      "raw": ["\"foo\""],
      "header_type": "item",
      "expected": "foo"
    },
    {
      "name": "whitespace string",
      "raw": ["\"   \""],
      "header_type": "item",
      "expected": "   "
    },
    {
      "name": "single quoted string",
      "raw": ["'foo'"],
      "header_type": "item",
      "expected": false
    },
    {
      "name": "unbalanced string",
      "raw": ["\"foo"],
      "header_type": "item",
      "expected": false
    },
    {
      "name": "string quoting",
      "raw": ["\"foo \\\"bar\\\" \\\\ baz\""],
      "header_type": "item",
      "expected": "foo \"bar\" \\ baz"
    },
    {
      "name": "bad string quoting",
      "raw": ["\"foo \\,\""],
      "header_type": "item",
      "expected": false
    },
    {
      "name": "ending string quote",
      "raw": ["\"foo \\\""],
      "header_type": "item",
      "expected": false
